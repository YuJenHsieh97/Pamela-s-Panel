
<h1>Edit Cohort Info</h1>
<br>
<br>
<%= form_for @cohort  do |c| %>
    <h1> Cohort Name</h1>
    <br>
    <%= c.text_field :name %>
    <h1> Start Date</h1>
    <br>
    <%= c.text_field :sDate %>
    <h1> End Date</h1>
    <br>
    <%= c.text_field :eDate %>
  
    <br>
  
  
    <br>
    <%= c.submit "Update" %>
  <% end %>